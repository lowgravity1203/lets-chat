<h1>This is the "choose your interests page bitch"</h1>

<h1><span style="color:red">Todo:</span>Add data to mongo</h1>

<form action="/channel/<%= currentUser._id %> " method="POST">
  <% interests.forEach(interest => { %>
  <label
    ><input
      type="checkbox"
      id="<%= interest.name %>"
      name="<%= interest.name %>"
      value="<%= interest.name %>"
    /><%= interest.name %></label
  >
  <br />
  <% }) %>
  <button type="submit">Submit</button>
</form>
